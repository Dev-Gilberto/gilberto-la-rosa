{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@styles/*": ["src/styles/*"],
      "@data/*": ["src/data/*"],
      "@layout/*": ["src/layouts/*"]
    },
    "strictNullChecks": true,
    "allowJs": true
  }
}
